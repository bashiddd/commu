<h2 class="likes-index-h2">いいね一覧</h2>
<div class="likes-index">
    <% @users.each do |user| %>
        <div class="like-user">
            <div class="like-user-img">
                <% if user.image_name=="default_user.png" %>
                    <img src="<%= "/user_images/default_user.png" %>">
                <% else %>
                    <img src="<%= "/user_images/#{user.image_name}" %>">
                <% end %>
            </div>
            <div class="like-user-name">
                <%= link_to("#{user.name}", "/users/#{user.id}", class: "like_user_name") %>
            </div>
        </div>
    <% end %>
</div>